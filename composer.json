{
  "name": "hazratmilad/db-secure-schema-state",
  "description": "SSL-capable schema dump state replacement for Laravel supporting MySQL, MariaDB, and PostgreSQL with secure connection options.",
  "keywords": ["laravel", "mysql", "mariadb", "postgresql", "postgres", "schema", "mysqldump", "pg_dump", "ssl"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Milad",
      "email": "undresspr.spy@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "laravel/framework": "^11|^12"
  },
  "autoload": {
    "psr-4": {
      "Milad\\DBSecureSchemaState\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Milad\\DBSecureSchemaState\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Milad\\DBSecureSchemaState\\DBSecureSchemaStateServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}